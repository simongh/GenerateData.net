<!DOCTYPE html>
<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<title>@SharedStrings.Title</title>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta name="description" content="@SharedStrings.MetaDescription" />
	<meta name="keywords" content="@SharedStrings.MetaKeywords" />

	@Styles.Render("~/content/smoothness")
	@RenderSection("css", false)
	@Scripts.Render("~/bundles/shared")
	@RenderSection("topscripts", false)
	@Scripts.Render("~/bundles/spinners")
	<!--[if lt IE 9]>
	@Scripts.Render("~/bundles/ie")
	<![endif]-->
</head>
<body>
	<header>
		@RenderSection("header")
	</header>
	@RenderBody()

	@{ Html.RenderPartial("footer");}
	@RenderSection("bottomscripts", false)
</body>
</html>
